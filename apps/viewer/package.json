{
  "name": "viewer",
  "version": "1.0.0",
  "scripts": {
    "dev": "dotenv -e ./.env -e ../../.env  -- next dev -p 3001",
    "build": "dotenv -e ./.env -e ../../.env -- next build",
    "start": "dotenv -e ./.env -e ../../.env -- next start -p 3001",
    "lint": "dotenv -e ./.env -e ../../.env -- next lint",
    "test": "dotenv -e ./.env -e ../../.env -- pnpm playwright test",
    "test:report": "pnpm playwright show-report"
  },
  "dependencies": {
    "models": "workspace:*",
    "@openbot/lib": "workspace:*",
    "@openbot/env": "workspace:*",
    "@openbot/react": "workspace:*",
    "@openbot/js2": "workspace:*",
    "next": "^13.4.6",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@playwright/test": "1.34.3",
    "@babel/core": "7.21.8",
    "tsconfig": "workspace:*",
    "@types/node": "20.2.3",
    "@types/react": "18.2.15",
    "dotenv-cli": "^7.2.1",
    "eslint": "8.41.0",
    "eslint-config-custom": "workspace:*",
    "next-runtime-env": "^1.6.2",
    "typescript": "5.0.4"
  }
}